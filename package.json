{
  "name": "kostl-next",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:client": "next dev -p 8000",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "dev:pathpida": "pathpida --enableStatic --watch",
    "build": "run-p build:client build:server",
    "prebuild:client": "npm run generate",
    "build:client": "next build",
    "build:server": "npm run build --prefix server",
    "generate": "run-p generate:*",
    "generate:aspida": "aspida",
    "generate:pathpida": "pathpida --enableStatic",
    "generate:server": "npm run generate --prefix server",
    "lint": "eslint --ext .ts,.js,.tsx . && prettier --check \"./**/*.{ts,js,tsx}\"",
    "lint:fix": "eslint --ext .ts,.js,.tsx . --fix && prettier --write \"./**/*.{ts,js,tsx}\"",
    "migrate:dev": "npm run migrate:dev --prefix server",
    "migrate:deploy": "npm run migrate:deploy --prefix server",
    "migrate:dev:createonly": "npm run migrate:dev:createonly --prefix server",
    "migrate:reset": "npm run migrate:reset --prefix server",
    "seed": "npm run seed --prefix server",
    "start": "run-p start:*",
    "start:client": "next start",
    "start:server": "npm start --prefix server",
    "pretypecheck": "npm run generate",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server"
  },
  "dependencies": {
    "@aspida/fetch": "^1.8.0",
    "@aspida/node-fetch": "^1.8.0",
    "@aspida/swr": "^1.8.0",
    "@chakra-ui/react": "^1.8.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@prisma/client": "^3.9.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.0",
    "framer-motion": "^6",
    "frourio": "^0.27.1",
    "memory-cache": "^0.2.0",
    "next": "^12.0.10",
    "pathpida": "^0.18.0",
    "prisma": "^3.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "swr": "^1.2.1"
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.10",
    "@types/memory-cache": "^0.2.2",
    "@types/node": "^17.0.15",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^7.24.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "notios": "^0.1.0",
    "pathpida": "^0.18.0",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "ts-loader": "~8.2.0",
    "typescript": "~4.5"
  }
}
