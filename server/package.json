{
  "name": "kostl-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:server": "node ./scripts/build.dev.js & node-dev --respawn index.js",
    "dev:frourio": "frourio --watch",
    "dev:prisma": "prisma generate --watch",
    "prebuild": "npm run generate",
    "build": "node ./scripts/build.prod.js",
    "generate": "run-p generate:*",
    "generate:frourio": "frourio",
    "generate:prisma": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev:createonly": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset",
    "seed": "prisma db seed --preview-feature",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "start": "cross-env NODE_ENV=production node index.js"
  },
  "dependencies": {
    "@kvs/memorystorage": "^2.1.1",
    "@kvs/storage": "^2.1.1",
    "@prisma/client": "^3.9.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.0",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.1",
    "fastify-cors": "^6.0.2",
    "fastify-helmet": "^7.0.1",
    "fastify-jwt": "^4.1.3",
    "fastify-multipart": "^5.3.0",
    "fastify-plugin": "^3.0.1",
    "fastify-static": "^4.5.0",
    "node-fetch": "^2.6.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/busboy": "^1.3.0",
    "@types/jest": "^27.4.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.19",
    "esbuild-node-externals": "^1.4.1",
    "frourio": "^0.27.1",
    "node-dev": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.9.1",
    "ts-loader": "~8.2.0",
    "ts-node": "^10.4.0",
    "typescript": "~4.5"
  }
}
